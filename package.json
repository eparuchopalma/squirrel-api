{
  "name": "squirrell-api",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node ./src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:up": "cross-env NODE_ENV=$npm_config_env sequelize-cli db:migrate",
    "migrate:undo": "cross-env NODE_ENV=$npm_config_env sequelize-cli db:migrate:undo",
    "seed:generate": "sequelize-cli seed:generate --name",
    "seed:up": "cross-env NODE_ENV=$npm_config_env sequelize-cli db:seed:all",
    "seed:undo": "cross-env NODE_ENV=$npm_config_env sequelize-cli db:seed:undo"
  },
  "keywords": [],
  "author": "Edgar Parucho Palma",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "express": "^5.1.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "@types/validator": "^13.15.2",
    "dotenv": "^17.2.1",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
